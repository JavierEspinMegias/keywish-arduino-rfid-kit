int motor1=5;
int motor2=6;
int val=9;
void setup(){
   Serial.begin(115200);
   pinMode(motor1,OUTPUT);
   pinMode(motor2,OUTPUT);
   pinMode(val,INPUT);
}
void loop()
{ 
    if (digitalRead(val)==HIGH)
    {
        digitalWrite(motor1,LOW);
        digitalWrite(motor2,HIGH);

    }
    else
    {
        digitalWrite(motor1,HIGH);
        digitalWrite(motor2,LOW); 
        Serial.print("bbbbbbbbbb \n");
    }
    delay(300);
 }
 
